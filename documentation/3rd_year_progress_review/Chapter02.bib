	keywords = {Internet, Drug Discovery, Pharmaceutical Preparations, Databases, Chemical, Quantitative Structure-Activity Relationship, Pharmacokinetics},
	pages = {D1091--1097},
	file = {Law et al. - 2014 - DrugBank 4.0 shedding new light on drug metabolis.pdf:/home/mpnme/.zotero/zotero/lhkcc87j.default/zotero/storage/VEIC2MGR/Law et al. - 2014 - DrugBank 4.0 shedding new light on drug metabolis.pdf:application/pdf}
