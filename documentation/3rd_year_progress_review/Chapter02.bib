	keywords = {Disease, Internet, Phenotype, Genomics, Databases, Chemical, Toxicogenetics, History, 21st Century},
	pages = {D914--920},
	file = {Davis et al. - 2015 - The Comparative Toxicogenomics Database's 10th yea.pdf:/home/mpnme/.zotero/zotero/lhkcc87j.default/zotero/storage/VAJ785QY/Davis et al. - 2015 - The Comparative Toxicogenomics Database's 10th yea.pdf:application/pdf}
