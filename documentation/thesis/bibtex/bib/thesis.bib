	keywords = {Humans, Animals, Proteins, Databases, Factual, Genome, Information Storage and Retrieval, Gene Expression, Japan},
	pages = {27--30},
	file = {Full Text:/home/mpnme/.zotero/zotero/lhkcc87j.default/zotero/storage/6EGHVU8A/Kanehisa and Goto - 2000 - KEGG kyoto encyclopedia of genes and genomes.pdf:application/pdf},
