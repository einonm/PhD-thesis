	note = {Number: Database issue},
	keywords = {Proteins, Pharmaceutical Preparations, Databases, Protein, Biochemical Phenomena, Stereoisomerism},
	pages = {D876--880},
	file = {Kuhn et al. - 2012 - STITCH 3 zooming in on protein-chemical interacti.pdf:/home/mpnme/.zotero/zotero/lhkcc87j.default/zotero/storage/5MHN853Y/Kuhn et al. - 2012 - STITCH 3 zooming in on protein-chemical interacti.pdf:application/pdf},
