	pages = {D198--201},
	file = {Full Text:/home/mpnme/.zotero/zotero/lhkcc87j.default/zotero/storage/RX578Y4Z/Liu et al. - 2007 - BindingDB a web-accessible database of experiment.pdf:application/pdf},
