	file = {Keiser et al. - 2007 - Relating protein pharmacology by ligand chemistry.pdf:/home/mpnme/.zotero/zotero/lhkcc87j.default/zotero/storage/QDXE2C7S/Keiser et al. - 2007 - Relating protein pharmacology by ligand chemistry.pdf:application/pdf},
