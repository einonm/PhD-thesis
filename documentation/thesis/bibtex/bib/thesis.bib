	year = {2022},
	pmid = {35469222},
	pmcid = {PMC9034918},
	note = {Place: United States},
	keywords = {Humans, *Non-alcoholic Fatty Liver Disease/drug therapy/genetics, Fibrosis, Liver/pathology},
	pages = {7606716},
	file = {Gao et al. - 2022 - Screening Potential Drugs for the Development of N.pdf:/home/mpnme/.zotero/zotero/lhkcc87j.default/zotero/storage/WAE8VHGJ/Gao et al. - 2022 - Screening Potential Drugs for the Development of N.pdf:application/pdf},
