	keywords = {Humans, Internet, Animals, Proteins, Computational Biology, Pharmaceutical Preparations, User-Computer Interface, Databases, Protein, Software, Models, Chemical, Information Storage and Retrieval, Protein Interaction Mapping, Drug Evaluation, Preclinical, Aspirin},
	pages = {D552--556},
	file = {Kuhn et al. - 2010 - STITCH 2 an interaction network database for smal.pdf:/home/mpnme/.zotero/zotero/lhkcc87j.default/zotero/storage/9DCM4PCP/Kuhn et al. - 2010 - STITCH 2 an interaction network database for smal.pdf:application/pdf},
